{
  "name": "expo-icp",
  "version": "0.1.7",
  "description": "CLI tool for Expo ICP integration",
  "type": "module",
  "main": "./dist/cli.js",
  "bin": {
    "expo-icp": "./dist/cli.js"
  },
  "preferGlobal": true,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run typecheck && vite build && chmod +x dist/cli.js",
    "test": "vitest --run",
    "typecheck": "tsc --noEmit",
    "test:coverage": "vitest run --coverage",
    "prepare": "npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/higayasuo/expo-icp-cli.git"
  },
  "bugs": {
    "url": "https://github.com/higayasuo/expo-icp-cli/issues"
  },
  "homepage": "https://github.com/higayasuo/expo-icp-cli#readme",
  "keywords": [
    "expo",
    "icp",
    "cli",
    "internet-computer",
    "dfinity"
  ],
  "author": "Yasuo Higa",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@vitest/coverage-v8": "^1.3.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vitest": "^1.3.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
